{
  "name": "infinite-autocomplete",
  "version": "4.0.0",
  "description": "Simple Parcel Sandbox",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "rimraf dist && parcel build src/index.js",
    "lint": "eslint src/",
    "prepublish": "npm run lint && npm run build"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "ghooks": "^2.0.4",
    "parcel-bundler": "^1.6.1",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run prepublish"
    }
  }
}
